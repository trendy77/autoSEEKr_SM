{
	"manifest_version": 2,
	"name": "clipSEEKr",
	"description": "SEEK with copy/paste.",
	"icons": {
		"16": "images/gear16.png",
		"32": "images/gear32.png",
		"48": "images/gear48.png",
		"128": "images/gear128.png"
	},
	"version": "2.0",
	"background": {
		"scripts": [
			"authorize.js",
			"userinfo.js",
			"background.js",
			"sidebar/sample_support.js"
		]
	},
	"browser_action": {
		"default_icon": {
			"16": "images/gear16.png",
			"32": "images/gear32.png",
			"48": "images/gear48.png",
			"128": "images/gear128.png"
		},
		"default_popup": "sidebar/index.html",
		"default_title": "clipSEEKr",
		"scripts": [
			"sidebar/materialize.min.js",
			"sidebar/mz.js",
			"sidebar/panel.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["https://*.seek.com.au/"],
			"js": ["tooltip.js", "clip.js"],
			"css": ["tooltip.css"]
		}
	],
	"permissions": [
		"storage",
		"tabs",
		"clipboardWrite",
		"clipboardRead",
		"*://seek.com.au/*",
		"*://accounts.google.com/*",
		"webNavigation",
		"identity",
		"proxy",
		"activeTab",
		"notifications",
		"contextMenus",
		"<all_urls>",
		"https://*.googleapis.com/*",
		"https://apis.google.com/js/api.js"
	],
	"web_accessible_resources": ["*"],
	"oauth2": {
		"client_id": "305649687039-4c7et3aiv8dupi81uprvt3lsnqkmi5b4.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/documents",
			"https://www.googleapis.com/auth/drive",
			"https://www.googleapis.com/auth/script.send_mail",
			"https://www.googleapis.com/auth/spreadsheets",
			"https://www.googleapis.com/auth/userinfo.email"
		]
	},
	"key": ""
}
